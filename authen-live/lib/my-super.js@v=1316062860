/*-- GET UNIVERSITY WISE RECIPT NO --*/
function generate_approx_receipt_no(value, cat, res_sec) {
    $(res_sec).val('');
    if(value != '') {
        $.ajax({
            type: "post",
            url: "select.php",
            data: {T1: 'get_approx_receipt_no', univr_id: value, selected_cat: cat},
            success: function (revert) {
                $(res_sec).val(revert);
            },
            error: function () {
                var dialog = $.dialog({
                    icon: 'fa fa-times',
                    type: 'red',
                    title: 'Error...',
                    content: 'We are having some Technical issue. Please try again later.',
                });
                dialog.open();
                setTimeout(function () {
                    dialog.close();
                }, jconfirm_timeout);
            }
        });
    }
}
