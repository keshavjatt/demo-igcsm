// RE-CAPTCHA
function re_captcha(form_id) {
    $(form_id + ' #save-btn').attr('disabled', 'disabled');
    $(form_id + ' #basic_captcha').mathCaptcha({
        successFunction: function () {
            $(form_id + ' .math-captcha-error').remove();
            $(form_id + ' #save-btn').removeAttr('disabled', 'disabled');
        },
        failFunction: function () {
            $(form_id + ' #save-btn').attr('disabled', 'disabled');
            /*if ($('.math-captcha-error').length < 1) {
                $('#basic_captcha').append("<p class='math-captcha-error'>Wrong result!</p>");
            }*/
        },
        formID: form_id
    });
}
