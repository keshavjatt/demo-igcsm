// ADD DIRECTOR REDIRECT
function add_director(id, box_title, box_text, red_url) {
    $.confirm({
        icon: 'fa fa-info-circle',
        title: box_title.replace(/\_/g, ' '),
        content: box_text.replace(/\_/g, ' '),
        type: 'blue',
        buttons: {
            confirm: function () {
                window.location.href = red_url + "?id=" + id;
            },
            cancel: function () {
                var dialog = $.dialog({
                    icon: 'fa fa-info-circle',
                    type: 'green',
                    title: 'Great...',
                    content: 'Action Canceled by you.',
                });
                dialog.open();
                setTimeout(function() {
                    dialog.close();
                }, jconfirm_timeout);
                //return false;
            }
        }
    });
}

// ACTIVATE ME
function activate_me(id, box_title, box_text, txbox) {
    $.confirm({
        icon: 'fa fa-info-circle',
        title: box_title.replace(/\_/g, ' '),
        content: box_text.replace(/\_/g, ' '),
        type: 'blue',
        buttons: {
            confirm: function () {
                var dialog = $.dialog({
                    content: function () {
                        var self = this;
                        return $.ajax({
                            type: "post",
                            url: "activate.php",
                            dataType: "json",
                            cache: false,
                            data: {T1: txbox, id: id},
                            success: function (revert) {
                                self.setIcon(revert.msg_icon);
                                self.setType(revert.msg_type);
                                self.setTitle(revert.msg_title);
                                self.setContent(revert.msg_text);
                                load_data();
                                setTimeout(function(){
                                    dialog.close();
                                }, jconfirm_timeout);
                            },
                            error: function () {
                                self.setIcon('fa fa-times');
                                self.setType('red');
                                self.setTitle('Error...');
                                self.setContent('We are having some Technical issue. Please try again later.' + txbox + id);
                                // Handle it your way
                            }
                            //Your code for AJAX Ends
                        });
                    },
                });
                dialog.open();
            },
            cancel: function () {
                var dialog = $.dialog({
                    icon: 'fa fa-info-circle',
                    type: 'green',
                    title: 'Great...',
                    content: 'Action Canceled by you.',
                });
                dialog.open();
                setTimeout(function() {
                    dialog.close();
                }, jconfirm_timeout);
                //return false;
            }
        }
    });
}

// BLOCK ME
function block_me(id, box_title, box_text, txbox) {
    $.confirm({
        icon: 'fa fa-info-circle',
        title: box_title.replace(/\_/g, ' '),
        content: box_text.replace(/\_/g, ' '),
        type: 'blue',
        buttons: {
            confirm: function () {
                var dialog = $.dialog({
                    content: function () {
                        var self = this;
                        return $.ajax({
                            type: "post",
                            url: "block.php",
                            dataType: "json",
                            cache: false,
                            data: {T1: txbox, id: id},
                            success: function (revert) {
                                self.setIcon(revert.msg_icon);
                                self.setType(revert.msg_type);
                                self.setTitle(revert.msg_title);
                                self.setContent(revert.msg_text);
                                load_data();
                                setTimeout(function(){
                                    dialog.close();
                                }, jconfirm_timeout);
                            },
                            error: function () {
                                self.setIcon('fa fa-times');
                                self.setType('red');
                                self.setTitle('Error...');
                                self.setContent('We are having some Technical issue. Please try again later.');
                                // Handle it your way
                            }
                            //Your code for AJAX Ends
                        });
                    },
                });
                dialog.open();
            },
            cancel: function () {
                var dialog = $.dialog({
                    icon: 'fa fa-info-circle',
                    type: 'green',
                    title: 'Great...',
                    content: 'Action Canceled by you.',
                });
                dialog.open();
                setTimeout(function() {
                    dialog.close();
                }, jconfirm_timeout);
                //return false;
            }
        }
    });
}


// TRASH ME
function trash_me(id, box_title, box_text, txbox) {
    $.confirm({
        icon: 'fa fa-info-circle',
        title: box_title.replace(/\_/g, ' '),
        content: box_text.replace(/\_/g, ' '),
        type: 'blue',
        buttons: {
            confirm: function () {
                var dialog = $.dialog({
                    content: function () {
                        var self = this;
                        return $.ajax({
                            type: "post",
                            url: "trash.php",
                            dataType: "json",
                            cache: false,
                            data: {T1: txbox, id: id},
                            success: function (revert) {
                                self.setIcon(revert.msg_icon);
                                self.setType(revert.msg_type);
                                self.setTitle(revert.msg_title);
                                self.setContent(revert.msg_text);
                                load_data();
                                setTimeout(function(){
                                    dialog.close();
                                }, jconfirm_timeout);
                            },
                            error: function () {
                                self.setIcon('fa fa-times');
                                self.setType('red');
                                self.setTitle('Error...');
                                self.setContent('We are having some Technical issue. Please try again later.');
                                // Handle it your way
                            }
                            //Your code for AJAX Ends
                        });
                    },
                });
                dialog.open();
            },
            cancel: function () {
                var dialog = $.dialog({
                    icon: 'fa fa-info-circle',
                    type: 'green',
                    title: 'Great...',
                    content: 'Action Canceled by you.',
                });
                dialog.open();
                setTimeout(function() {
                    dialog.close();
                }, jconfirm_timeout);
                //return false;
            }
        }
    });
}

/////////////////////////////////// VIEW ME//////////////////////////////////
function view_me(id, id_role) {
    $("#myModal").html('');
    $.ajax({
        type: "post",
        url: "view.php",
        data: {T1: id_role, id: id},
        beforeSend: function () {
            $('.preloader-sec').show();
        },
        success: function (revert) {
            // $("#pre_load").addClass("hidden");
            $('#my_modal').html(revert);
            $('#my_modal').modal('show', {backdrop: 'static'});
            $('.preloader-sec').hide();
        },
        error: function () {
            var dialog = $.dialog({
                icon: 'fa fa-times',
                type: 'red',
                title: 'Error...',
                content: 'We are having some Technical issue. Please try again later.',
            });
            dialog.open();
            setTimeout(function() {
                dialog.close();
            }, jconfirm_timeout);
        }
    });
}

