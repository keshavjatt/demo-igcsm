$(document).ready(function(){
    // $('body').css('zoom','90%'); /* Webkit browsers */
    // $('body').css('zoom','0.9'); /* Other non-webkit browsers */
    // $('body').css('-moz-transform', 'scale(0.9, 0.9)'); /* Moz-browsers */

    $('input[type="text"]').on('blur change', function() {
        
        var str = $(this).val();
        var res = str.toUpperCase();
        $(this).val(res);
    });

    $('input[type="text"]').on('blur change', function() {
        var str = $(this).val();
        var res = str.replace(/\s+/g, " ");
        $(this).val(res);
    });

    $('.no-effect').on('keyup change', function() {
        var str = $(this).val();
        var res = str.toLowerCase();
        $(this).val(res);
    });
});

function pad (str, max) {
    str = str.toString();
    return str.length < max ? pad("0" + str, max) : str;
}

function parseDate(str) {
    var mdy = str.split('-');
    return new Date(mdy[2], mdy[1]-1, mdy[0]);
}

/********* GET STATE --*/
function get_states(res_sec, preselect) {
    $.ajax({
        type: "post",
        url: "select.php",
        data: {T1: 'get_states', preselect: preselect},
        success: function (revert) {
            $(res_sec).html(revert).selectpicker('refresh');
        },
        error: function () {
            var dialog = $.dialog({
                icon: 'fa fa-times',
                type: 'red',
                title: 'Error...',
                content: 'We are having some Technical issue. Please try again later.',
            });
            dialog.open();
            setTimeout(function() {
                dialog.close();
            }, jconfirm_timeout);
        }
    });
}


