function get_section(res_sec, course_id, preselect) {
    $.ajax({
        type: "post",
        url: "select.php",
        data: {T1: 'get_section', preselect: preselect,course_id:course_id},
        success: function (revert) {
            $(res_sec).html(revert).selectpicker('refresh');
        },
        error: function () {
            var dialog = $.dialog({
                icon: 'fa fa-times',
                type: 'red',
                title: 'Error...',
                content: 'We are having some Technical issue. Please try again later.',
            });
            dialog.open();
            setTimeout(function() {
                dialog.close();
            }, jconfirm_timeout);
        }
    });
}

