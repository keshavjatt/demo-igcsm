function get_Course_wise_price() {
    var course_id   =   [];
    $('.item_id').each(function () { 
        if($(this).val() !=''){
            var student_id  =  $(this).parents('.enroll_student').find('.student');
            console.log(student_id.is(':checked'));
            if(student_id.is(':checked')){
                course_id.push({student_id: student_id.val(),courseid: $(this).val()});
            }
            
        }
    });
    if(course_id.length > 0){
        $.ajax({
            type: "post",
            url: "select.php",
            dataType: "json",
            data: {T1: 'get_Course_wise_price', course_id: course_id},
            success: function (revert) {
                $('#amt_to_pay_enroll').html(revert.get_gst_amount);
                $('#data_student').val(btoa(revert.data));
                $('#fran_comm').val(revert.fran_comm);
                $('#student_count').val(revert.student_count);
                $('#total_amount').val(revert.get_gst_amount);
            },
            error: function () {
                var dialog = $.dialog({
                    icon: 'fa fa-times',
                    type: 'red',
                    title: 'Error...',
                    content: 'We are having some Technical issue. Please try again later.',
                });
                dialog.open();
                setTimeout(function() {
                    dialog.close();
                }, jconfirm_timeout);
            }
        });
        return true;
    }
}





