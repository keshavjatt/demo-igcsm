function get_specific_fee(center_code, course_code, part_2, resrv_cat) {
    $.ajax({
        type: "post",
        url: "select.php",
        data: {T1: 'get_specific_fee', center_code: $(center_code).val(), course_code: $(course_code).val(), part_2: $(part_2).val(), resrv_cat: $(resrv_cat).val()},
        success: function (revert) {
            $(course_fee).val(revert);
        },
        error: function () {
            var dialog = $.dialog({
                icon: 'fa fa-times',
                type: 'red',
                title: 'Error...',
                content: 'We are having some Technical issue. Please try again later.',
            });
            dialog.open();
            setTimeout(function() {
                dialog.close();
            }, jconfirm_timeout);
        }
    });
}

function check_username(id, usr_cat) {
    var val = $('#'+id).val();
    $.ajax({
        type: "post",
        url: "select.php",
        data: {T1: 'check_username', usr_cat: usr_cat, username: val},
        success: function (revert) {
            if(revert > 0) {
                $('#'+id).val('').focus();

                var dialog = $.dialog({
                    icon: 'fa fa-warning',
                    type: 'orange',
                    title: 'Warning...',
                    content: 'Username already exist in our Records.',
                });
                dialog.open();
            }
        },
        error: function () {
            var dialog = $.dialog({
                icon: 'fa fa-times',
                type: 'red',
                title: 'Error...',
                content: 'We are having some Technical issue. Please try again later.',
            });
            dialog.open();
            setTimeout(function() {
                dialog.close();
            }, jconfirm_timeout);
        }
    });
}
// EDIT ME REDIRECT
function edit_me(id, box_title, box_text, red_url) {
    $.confirm({
        icon: 'fa fa-info-circle',
        title: box_title.replace(/\_/g, ' '),
        content: box_text.replace(/\_/g, ' '),
        type: 'blue',
        buttons: {
            confirm: function () {
                // $('a.edit').attr('target', '_blank');
                // window.location.href = red_url + "?id=" + id;
                window.open(red_url + "?id=" + id , '_blank');
                // target = "_blank";
            },
            cancel: function () {
                var dialog = $.dialog({
                    icon: 'fa fa-info-circle',
                    type: 'green',
                    title: 'Great...',
                    content: 'Action Canceled by you.',
                });
                dialog.open();
                setTimeout(function() {
                    dialog.close();
                }, jconfirm_timeout);
                //return false;
            }
        }
    });
}


function edit_photo(enrolment_no) {
    $('#loader_'+enrolment_no).removeClass('hide');
    $.ajax({
        type: "post",
        url: "view.php",
        data: {T1: 'get_photo_edit_form', enrolment_no: enrolment_no},
        success: function (revert) {
            $('#my_modal').html(revert);
            $('#my_modal').modal('show', {backdrop: 'static'});
            $('#loader_'+enrolment_no).addClass('hide');

            $('.profile_pic').filestyle({
                buttonName : 'btn-success',
                buttonText : 'CHOOSE'
            });
            $('#pro_pic_img').attr('src', $('#pic_file').val());
        },
        error: function () {
            toastMessenger.post({
                message: revert.msg,
                type: 'error',
                showCloseButton: true
            });
        }
    });
}

function edit_apli_photo(aplication_no) {
    $('#loader_'+aplication_no).removeClass('hide');
    $.ajax({
        type: "post",
        url: "view.php",
